GETTING INTEGER AND PRINTING 1ST AND LAST DIGIT
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int count=0;
		if(a<0){
		    a=-a;
		}
		int b=a%10;
		while(a>10){
		    a=a/10;
		}
		 System.out.println(b);
		 System.out.println(a);
	}
}
